# Maintainer: Maykel Moya <mmoya [at] mmoya [dot] org>

pkgname=logdy-bin
_pkgname="${pkgname%-*}"
pkgver=0.13.0
pkgrel=1
pkgdesc='Web based real-time log viewer. Stream ANY content to a web UI with autogenerated filters. Parse any format with TypeScript.'
arch=('x86_64')
url="https://logdy.dev/"
license=('Apache')
_filename="${_pkgname}_linux_amd64"
source=("https://github.com/logdyhq/logdy-core/releases/download/v${pkgver}/${_filename}")
sha256sums=('9a5c5ae93ac0b6a3a0f17a984297375a4d18c5c63102875f952e9861f6e64c4f')

package() {
  install -Dm755 "${_filename}" "${pkgdir}/usr/bin/${_pkgname}"
}

